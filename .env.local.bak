
# Local Development Environment - EXAMPLE ONLY
# SECURITY WARNING: This file contained real secrets and has been sanitized
# Use .env.example to create your local configuration

# Active Directory Configuration (placeholder values - REPLACE WITH YOUR VALUES)
AD_SERVER=your-domain-controller.local
AD_DOMAIN=yourdomain.local
AD_BASE_DN=DC=yourdomain,DC=local
AD_USERNAME=CN=service-account,CN=Users,DC=yourdomain,DC=local
AD_PASSWORD=your-secure-password

# Credential Encryption - GENERATE NEW KEYS FOR PRODUCTION
CREDENTIAL_ENCRYPTION_SALT=generate-new-salt-with-openssl-rand-hex-32
CREDENTIAL_ENCRYPTION_KEY=generate-new-key-minimum-32-characters-with-openssl-rand-hex-32

# Database Configuration - USE SECURE PASSWORDS IN PRODUCTION
POSTGRES_PASSWORD=your-secure-db-password
DATABASE_URL=postgresql://postgres:your-secure-db-password@localhost:5432/reporting

# Redis Configuration - USE SECURE PASSWORDS IN PRODUCTION
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your-secure-redis-password
REDIS_URL=redis://:your-secure-redis-password@localhost:6379

# Azure AD Configuration
AZURE_TENANT_ID=placeholder
AZURE_CLIENT_ID=placeholder
AZURE_CLIENT_SECRET=placeholder

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=json
LOG_MAX_SIZE=10m
LOG_MAX_FILES=5
ENABLE_DB_LOGS=true
DB_LOG_LEVEL=info
LOG_DB_QUERIES=false
LOG_RETENTION_DAYS=30
